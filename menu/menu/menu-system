echo -e "$NC   [${Lgreen}01${NC}]. INSTALL NOOBZVPN    $NC[${Lgreen}09${NC}]. REBOOT VPS"
echo -e "$NC   [${Lgreen}02${NC}]. INSTALL UDP CUSTOM  $NC[${Lgreen}10${NC}]. RENEW DOMAIN"
